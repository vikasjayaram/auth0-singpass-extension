{
    "title": "Auth0 Singpass Proxy Extension",
    "name": "auth0-singpass-extension",
    "version": "1.0.0",
    "author": "vikasjayaram",
    "useHashName": false,
    "description": "singpass proxy endpoint to support private key jwt / client assertion",
    "type": "application",
    "category": "end_user",
    "logoUrl": "https://cdn.auth0.com/styleguide/components/1.0.8/media/logos/img/badge.png",
    "initialUrlPath": "/",
    "keywords": [
      "auth0",
      "extension"
    ],
    "secrets": {
      "AUTH0_CUSTOM_DOMAIN": {
        "description": "YOUR_CUSTOM_DOMAIN",
        "example": "auth.oauth101.net",
        "required": true
      },
      "SINGPASS_TOKEN_ENDPOINT": {
        "description": "Token Endpoint",
        "example": "https://stg-id.singpass.gov.sg/token",
        "required": true
      },
      "SINGPASS_ISSUER": {
        "description": "Issuer",
        "example": "https://stg-id.singpass.gov.sg",
        "required": true
      },
      "SINGPASS_SIGNING_ALG": {
        "description": "Signing Algorithm",
        "type": "select",
        "allowMultiple": false,
        "default": "true",
        "options": [
          {
            "value": "ES256",
            "text": "ES256"
          }
        ]
      },
      "SINGPASS_JWKS_ENDPOINT": {
        "description": "IDP JSON Web Key endpoint",
        "example":  "https://stg-id.singpass.gov.sg/.well-known/keys",
        "required": true
      },
      "RELYING_PARTY_JWKS_ENDPOINT": {
        "description": "Relying Party JSON Web Key",
        "example": "https://rp.com/.well-known/jwks.json",
        "required": true
      },
      "RELYING_PARTY_PRIVATE_KEY": {
        "description": "Relying Party Private Key",
        "required": true
      },
      "RELYING_PARTY_KID": {
        "description": "Relying Party KID",
        "required": true
      },
      "SINGPASS_CLIENT_ID": {
        "description": "Registered client_id",
        "required": true
      },
      "SINGPASS_AUDIENCE": {
        "description": "Audience",
        "example": "https://stg-id.singpass.gov.sg",
        "required": true
      } 
    }
  }  